#include <iostream>
using namespace std;
int main() {
// Print greeting
cout << "Hello Inventory Systems!"<< endl;
// Demonstrate escape characters
cout <<"This is on a new line.\n";
cout <<"This line is tabbed in.\tSee?"<< endl;
// Output sizes of data types 
cout << "Size of int:" << sizeof(int)<<"bytes"<<endl;
cout << "Size of my float:"<< sizeof(float)<<"bytes"<<endl;
cout <<"Size of char:"<< sizeof(char)<<"bytes"<< endl;
return 0;
}
